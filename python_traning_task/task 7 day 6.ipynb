   "source": [
    "def check (s):\n",
    "    c=0\n",
    "    l=0\n",
    "    for x in s:\n",
    "        if x.isupper()==True:\n",
    "            c+=1\n",
    "        if x.islower()==True :\n",
    "            l+=1\n",
    "    print('number of upper character :',c)\n",
    "    print('number of lower character :',l)"
   ]
  },
